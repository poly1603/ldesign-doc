{
  "name": "ldoc-theme-minimal",
  "version": "0.1.0",
  "description": "极简风格的 LDoc 主题",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./styles": "./dist/styles/index.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "ldoc",
    "ldoc-theme",
    "minimal",
    "documentation",
    "theme"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@ldesign/doc": ">=1.0.0",
    "vue": ">=3.3.0"
  },
  "devDependencies": {
    "@ldesign/doc": "workspace:*",
    "@vitejs/plugin-vue": "^5.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vue": "^3.4.0",
    "vue-tsc": "^2.0.0"
  }
}
